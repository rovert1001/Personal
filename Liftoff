class Main {
  public static void main(String[] args) {

    System.out.println("3");
    System.out.println("2");
    System.out.println("1");
    System.out.println("");
    
    for(int i = 0; i < 100000; i+=1000) {
        System.out.println("");
        System.out.println("Your altitiude is" + i);
    }
  }
}
