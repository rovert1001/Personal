import java.util.Scanner;
import java.util.Random;
class Main {
  public static void main(String[] args) {

    System.out.println("3");
    System.out.println("2");
    System.out.println("1");
    System.out.println("");
    System.out.println("We have liftoff!");
    
    for(int i = 0; i < 100000; i+=1000) {
        Scanner input = new Scanner(System.in);
        String response = input.nextLine();
        
        if(response.equalsIgnoreCase("height")) {
            System.out.println("");
            System.out.println("Your altitiude is " + i);
            System.out.println("");
        } else if(response.equalsIgnoreCase("down")) {
            System.out.println("");
            System.out.println("Your ship starts do decelerate once you move it.");
            System.out.println("After a few seconds you start picking up speed towards the ground.");
            System.out.println("");
            break;
        } else if(response.equalsIgnoreCase("up")) {
            System.out.println("");
            System.out.println("You try to move the ship to porgrade, but you soon relize that is impossible because you are already moving in the upward direction");
            System.out.println("");
        } else if {
        } else {
            System.out.println("The ship pushes higher as you take no action.");
        }
      
    }
  }
}
